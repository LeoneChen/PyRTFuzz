<?xml version="1.0" ?>
<apisepc version="1.0">
    <library name="email">
        <module name="charset">
            <class name="Charset" init="obj = Charset()" args="">
                <api name="get_body_encoding">
                        <expr>ret = obj.get_body_encoding()</expr>
                        <parameters>[]</parameters>
                        <return>['ret:base64|7bit']</return>
                        <dependences>  </dependences>
                </api>

                <api name="header_encode">
                        <expr>Ch.header_encode(str)</expr>
                        <parameters>['str:string']</parameters>
                        <return>[]</return>
                        <dependences>  </dependences>
                </api>
            </class>
        </module>

        <module name="contentmanager">
            <api name="header_encode">
                <expr>header_encode(str)</expr>
                <parameters>['str:string']</parameters>
                <return>[]</return>
                <dependences>  </dependences>
            </api>
        </module >

        <errors>
            <exception>email.errors.MessageError</exception>
            <exception>email.errors.MessageParseError</exception>
            <exception>email.errors.BoundaryError</exception>
            <exception>email.errors.MultipartConversionError</exception>
        </errors>
        
    </library>
</apisepc>